{
  "name": "aamva-id-maker",
  "version": "1.0.0",
  "description": "AAMVA-compliant ID/Driver License generator following DL/ID Card Design Standard 2025 v1.0",
  "main": "src/main.js",
  "bin": {
    "aamva-id-maker": "./src/main.js"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "node src/main.js --validate-only",
    "interactive": "node src/main.js --interactive",
    "generate:dl": "node src/main.js --template dl --format horizontal",
    "generate:id": "node src/main.js --template id --format horizontal",
    "generate:vertical": "node src/main.js --template dl --format vertical",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "aamva",
    "driver-license",
    "identification",
    "id-card",
    "pdf417",
    "barcode",
    "security",
    "government",
    "real-id"
  ],
  "author": "ID Maker Development Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "pdf417-generator": "^1.1.0",
    "node-pdf417": "^1.0.0",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.50.0",
    "@eslint/js": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-node": "^11.1.0",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/aamva-id-maker.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/aamva-id-maker/issues"
  },
  "homepage": "https://github.com/your-username/aamva-id-maker#readme"
}